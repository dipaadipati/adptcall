<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="room.css">
  <link rel="stylesheet" href="coreui/css/coreui.min.css">
  <script defer src="peerjs.min.js"></script>
  <script src="/socket.io/socket.io.js" defer></script>
  <script src="jquery-3.6.0.min.js" defer></script>
  <script src="coreui/js/coreui.js" defer></script>
  <!--<script src="bootstrap/js/bootstrap.js" defer></script>-->
  <title>Call Room</title>
  <style>
    #video-grid {
      display: grid;
      grid-template-columns: repeat(auto-fill, 300px);
      grid-auto-rows: 300px;
    }
    
    video {
      width: 100%;
      height: 100%;
      object-fit: cover;
    }
  </style>
</head>
<body>
  <div id="video-grid"></div>
  <div class="chat_window">
    <div class="top_menu">
        <div class="buttons">
            <div class="button close"></div>
            <div class="button minimize"></div>
            <div class="button maximize"></div>
        </div>
        <div class="title">
			Chat
			<!--<div class="change-name" style="float: right"><input type="text" class="form-control" placeholder="Masukkan Nama" id="nama"></div>-->
		</div>
    </div>
	<div id="setName">
	  <div class="nameC">
		<form method="post">
			<div class="row d-flex justify-content-center">
				<input class="form-control" placeholder="Masukkan Nama" autocomplete="off" name="myname" id="namaku">
			</div>
			<div class="row d-flex justify-content-center">
				<button type="submit" class="btn btn-primary col-sm-4">Masuk</button>
			</div>
		</form>
	  </div>
	</div>
</div>
<div class="message_template">
    <li class="message">
        <div class="avatar"></div>
        <div class="text_wrapper">
			<h6 class="name"></h6>
            <div class="text"></div>
        </div>
    </li>
</div>
</body>
</html>